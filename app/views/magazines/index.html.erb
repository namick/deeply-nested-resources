<h1>Listing magazines</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @magazines.each do |magazine| %>
  <tr>
    <td><%= magazine.name %></td>
    <td><%= link_to 'Show', [@publisher, magazine] %></td>
    <td><%= link_to 'Edit', edit_publisher_magazine_path(@publisher, magazine) %></td>
    <td><%= link_to 'Destroy', [@publisher, magazine], method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Magazine', new_publisher_magazine_path %>
